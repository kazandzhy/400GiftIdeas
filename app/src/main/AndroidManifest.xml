<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kazandzhy.com">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="kazandzhy.com.ContactUsActivity"
                  android:windowSoftInputMode="stateHidden"></activity>
        <activity android:name="kazandzhy.com.RandomGiftActivity"
                  android:configChanges="orientation|screenSize"/>
        <activity android:name="kazandzhy.com.GiftAdviceActivity" />
        <activity android:name="kazandzhy.com.AboutActivity" />
        <activity android:name="kazandzhy.com.GiftIdeaActivity" />
        <activity android:name="kazandzhy.com.Interests4_12Activity" />
        <activity android:name="kazandzhy.com.GiftIdeasActivity" />
        <activity android:name="kazandzhy.com.InterestsActivity" />
        <activity android:name="kazandzhy.com.GenderActivity" />
        <activity android:name="kazandzhy.com.ExtrasActivity" />
        <activity android:name="kazandzhy.com.GiftsByEventActivity" />
        <activity android:name="kazandzhy.com.GiftsByAgeActivity" />
        <activity android:name="kazandzhy.com.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>